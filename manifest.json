{
	"name": "Bugzilla Counter",
	"version": "1.0",
	"description": "A set of tools for displaying work in Bugzilla",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" },
	"permissions": [
		"tabs",
		"notifications",
		"https://bugzilla.redhat.com/*"
	],
	"options_page": "options.html",
	"background_page": "background.html",
	"browser_action": {
		"default_icon": "icon48.png",
		"popup": "popup.html"
	}
}
